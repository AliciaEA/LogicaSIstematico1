Proceso VelocidadObjeto
	Definir velocidad Como Real;
	Definir distancia Como Real;
	Definir tiempo Como Real;
	Escribir 'Escribir la distancia recorrida en kilometros';
	Leer distancia;
	Escribir 'Escribir el tiempo en que se recorrio la distancia en horas';
	Leer tiempo;
	velocidad <- distancia/tiempo;
	Escribir 'Si se recorrieron ', distancia, ' kilometros en ', tiempo, ' horas la velocidad en km/h es de ', velocidad;
FinProceso
